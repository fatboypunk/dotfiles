git_remove_locals () {
  git branch --merged | grep -v "\*" | xargs -n 1 git branch -D;
}

driveby() {
  lsof -i tcp:$1 | awk 'NR!=1 {print $2}' | xargs kill
  printf "Killed everything on port $1\n"
}

